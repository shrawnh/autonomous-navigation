{
  "name": "proto",
  "displayName": "proto-language-support",
  "description": "Webots vs code proto file language support",
  "version": "0.0.5",
  "scripts": {
    "vsce": "../node_modules/.bin/vsce"
  },
  "engines": {
    "vscode": "^1.85.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "proto",
        "aliases": [
          "PROTO",
          "proto"
        ],
        "extensions": [
          ".proto",
          ".wrl",
          ".wbt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "proto",
        "scopeName": "source.proto",
        "path": "./syntaxes/proto.tmLanguage.json"
      }
    ]
  }
}